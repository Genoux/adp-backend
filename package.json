{
  "name": "adp-backend",
  "version": "1.4.2",
  "description": "Backend for Aram Draft Pick",
  "main": "dist/index.js",
  "scripts": {
    "start": "node -r module-alias/register ./dist/index.js",
    "build": "tsc && tsc-alias",
    "dev": "nodemon --watch src -e ts,json --exec ts-node src/index.ts",
    "dev:docker": "docker-compose -f docker-compose.dev.yml up",
    "railway": "railway run npm start",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "prettier": "prettier --write \"src/**/*.ts\""
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "keywords": [],
  "author": "John Barry",
  "license": "",
  "dependencies": {
    "@supabase/supabase-js": "^2.24.0",
    "cors": "^2.8.5",
    "cronstrue": "^2.50.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.1.3",
    "easytimer.js": "^4.5.4",
    "express": "^4.17.1",
    "module-alias": "^2.2.3",
    "node-cron": "^3.0.3",
    "pusher": "^5.2.0",
    "socket.io": "^4.1.3"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@types/express": "^4.17.11",
    "@types/jest": "^29.5.2",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^16.0.0",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "jest": "^29.5.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "socket.io-client": "^4.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
