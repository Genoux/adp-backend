{
  "name": "adp-backend",
  "version": "1.5.0",
  "description": "Backend for Aram Draft Pick",
  "main": "dist/index.js",
  "scripts": {
    "start": "node -r module-alias/register ./dist/index.js",
    "build": "npm run compile",
    "compile": "tsc && npm run copy-views",
    "copy-views": "cp -r src/views dist/",
    "dev": "nodemon --watch src -e ts,json --exec ts-node src/index.ts",
    "dev:docker": "docker-compose -f docker-compose.dev.yml up",
    "railway": "railway run npm start",
    "render:build": "npm run compile",
    "render:start": "npm start",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "test:custom": "ts-node src/runTests.ts",
    "test:race": "ts-node src/runTests.ts --race-conditions",
    "test:sync": "ts-node src/runTests.ts --sync"
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "keywords": [],
  "author": "John Barry",
  "license": "",
  "dependencies": {
    "@babel/cli": "^7.25.6",
    "@babel/core": "^7.25.2",
    "@babel/node": "^7.25.0",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@supabase/supabase-js": "^2.45.3",
    "babel-loader": "^9.1.3",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "cron-parser": "^4.9.0",
    "cronstrue": "^2.50.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "easytimer.js": "^4.6.0",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "inspector-gadget": "^1.0.0",
    "module-alias": "^2.2.3",
    "node-cron": "^3.0.3",
    "pusher": "^5.2.0",
    "socket.io": "^4.7.5",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/cors": "^2.8.17",
    "@types/ejs": "^3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^22.5.2",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "eslint": "^9.9.1",
    "gulp": "^5.0.0",
    "gulp-copy": "^4.0.1",
    "gulp-typescript": "^6.0.0-alpha.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "socket.io-client": "^4.7.5",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  }
}
